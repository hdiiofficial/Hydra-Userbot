

{
      "name": "Hydra-UserBot Pyrogram",
      "description": "Hydra the most powerful Userbot",
      "logo": "https://te.legra.ph/file/18226e0514d54b1bb735a.jpg",
      "keywords": [
         "telegram",
         "Powerfull",
         "Userbot",
         "Hydra UB",
         "Hydra"
      ],   
   "repository": "https://github.com/hdiiofficial/Hydra-Userbot",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": true,
         "value": ""
      },
      "API_ID": {
         "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
       "STRING_SESSION": {
         "description": "Pyrogram Based String Session (2nd ID) [ From https://repl.it/@SpEcHiDe/GenerateStringSession ]",
         "required": true,
         "value": ""
      },
       "HEROKU_API_KEY": {
         "description": "Your Heroku API_KEY",
         "required": true,
         "value": ""
      },
       "HEROKU_APP_NAME: {
         "description": "Your Heroku APP_NAME",
         "required": true,
         "value": ""
      }
      
   }
}

